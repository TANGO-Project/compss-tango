#!/bin/bash -e

  runtimelog=$1

  submitList=$(cat $runtimelog | grep "New Job" | tr ":" "\t" | awk {' print $NF '} | tr ")" "\t" | awk {' print $1 '})
  hostList=$(cat $runtimelog  | grep "JobManager" | grep "Target host" | tr ":" "\t" | awk {' print $11 '})
  
  position=1
  while  read -r task; do
    resource=$(echo $hostList | cut -d " " -f $position)
    echo "$task $resource"
    position=$((position+1))
  done <<< "$submitList"
