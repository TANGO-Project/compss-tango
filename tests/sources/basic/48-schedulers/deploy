#!/bin/bash -e
  
  #Obtain parameter
  source_base_dir=$1
  target_base_dir=$2
  
  #Copy files
  cp -f ${source_base_dir}/target/48-schedulers.jar ${target_base_dir}/schedulers.jar   
                  
  cp -f ${source_base_dir}/project.xml ${target_base_dir}/project.xml                  
  cp -f ${source_base_dir}/resources.xml ${target_base_dir}/resources.xml
  
  cp -f ${source_base_dir}/execution ${target_base_dir}/execution
  cp -f ${source_base_dir}/result_defaultScheduler ${target_base_dir}/result_defaultScheduler
  cp -f ${source_base_dir}/result_fifoScheduler ${target_base_dir}/result_fifoScheduler
  cp -f ${source_base_dir}/result_lifoScheduler ${target_base_dir}/result_lifoScheduler
  cp -f ${source_base_dir}/result_fifoDataScheduler ${target_base_dir}/result_fifoDataScheduler
  cp -f ${source_base_dir}/result_dataScheduler ${target_base_dir}/result_dataScheduler

  
