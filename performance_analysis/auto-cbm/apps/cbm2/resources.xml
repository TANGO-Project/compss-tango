    <Service wsdl="http://localhost:8080/groupService?wsdl">
       <Name>GroupService</Name>
       <Namespace>http://groupService</Namespace>
       <Port>GroupServicePort</Port>
    </Service>	

    <CloudProvider name="BSC">
        <Server>https://bscgrid20:11443/</Server>
        <Connector>integratedtoolkit.connectors.test.Test</Connector>
        <ImageList>
            <Image name="uuid_test_XX_unstable_XX">
                <Architecture>amd64</Architecture>
                <OSType>Linux</OSType>
                <ApplicationSoftware><Software>Hmmer</Software></ApplicationSoftware>
                <Adaptors>
                    <Adaptor name="integratedtoolkit.nio.master.NIOAdaptor">
                        <MinPort>43100</MinPort>
                        <MaxPort>43110</MaxPort>
                    </Adaptor>
                </Adaptors>
            </Image>
        </ImageList>
        <CreationTime>120</CreationTime> <!-- In seconds -->
        <InstanceTypes>
           <Resource Name="small">
               <Capabilities>
                   <Processor>
                       <Architecture>amd64</Architecture>
                       <CoreCount>1</CoreCount>
                   </Processor>
                   <StorageElement>
                       <Size>10.0</Size>
                   </StorageElement>
                   <Memory>
                       <PhysicalSize>2.0</PhysicalSize>
                   </Memory>
               </Capabilities>
               <Price>0.085</Price>
           </Resource>
           <Resource Name="medium">
               <Capabilities>
                   <Processor>
                       <Architecture>amd64</Architecture>
                       <CoreCount>2</CoreCount>
                   </Processor>
                   <StorageElement>
                       <Size>10.0</Size>
                   </StorageElement>
                   <Memory>
                       <PhysicalSize>3.0</PhysicalSize>
                   </Memory>
               </Capabilities>
               <Price>0.212</Price>
           </Resource>
        </InstanceTypes>
    </CloudProvider>    	  

